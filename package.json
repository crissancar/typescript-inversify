    "@types/uuid": "^8.3.1",
    "@types/uuid-validate": "^0.0.1",
    "awilix": "^4.3.4",
    "awilix-express": "^4.0.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-promise-router": "^4.1.0",
    "glob": "^7.1.7",
    "http-status": "^1.5.0",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5",
    "uuid": "^8.3.2",
    "uuid-validate": "^0.0.3"
  },
